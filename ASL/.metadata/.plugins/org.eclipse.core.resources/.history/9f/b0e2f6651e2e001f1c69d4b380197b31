package in.dsa.string.simple;

public class Dummy {

	public static void main(String[] args) {
		
		String s="aabaaaa";
		for(int i =0; i<s.length()-2;i++){

            if(s.charAt(i)==s.charAt(i+1) && s.charAt(i+1)==s.charAt(i+2)){
                s = s.replace(s.charAt(i),' ');
               System.out.println(s); 
            }
        }

		System.out.println(s);
	}

}
